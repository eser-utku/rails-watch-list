<p id="notice"><%= notice %></p>

<h1>Welcome to Lists Page</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.name %></td>
        <td><%= link_to 'Show', list %></td>
        <td><%= link_to 'Edit', edit_list_path(list) %></td>
        <%# <td><%= link_to 'Delete', list, method: :delete, data: { confirm: 'You are deleting a list!' }%>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

